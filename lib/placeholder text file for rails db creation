rails generate draft:resource <MODEL_NAME_SINGULAR> <COLUMN_1_NAME>:<COLUMN_1_DATATYPE> <COLUMN_2_NAME>:<COLUMN_2_DATATYPE> # etc


rails generate draft:resource neighborhood name:string city_id:integer description:text wiki_link:string lat:decimal lng:decimal
rails generate draft:resource city name:string state:string description:string wiki_link:string lat:decimal lng:decimal 
rails generate draft:resource reason user_id:integer description:string link:string upvotes:integer downvotes:integer neighborhood_id_1:integer neighborhood_id_2:integer

rails generate draft:account user first_name:string last_name:string 

rails db:migrate RAILS_ENV=development
